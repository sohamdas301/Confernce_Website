  <body>
    <div class="over">
      <div class="container" id="container">
        <div class="form-container sign-up-container">
          <form action="#">
            <h1>Create Account</h1>
            <div class="social-container">
              <a href="#" class="social"><i class="fab fa-facebook-f"></i></a>
              <a href="#" class="social"><i class="fab fa-google"></i></a>
              <a href="#" class="social"><i class="fab fa-linkedin-in"></i></a>
            </div>
            <span>or use your email for registration</span>
            <div class="username">
              <input
                type="text"
                class="username-input"
                placeholder="username"
              />
            </div>
            <div class="email">
              <input type="email" class="email-input" placeholder="email" />
            </div>
            <div class="password">
              <input
                type="password"
                class="pass-input"
                placeholder="password"
              />
            </div>
            <button style="margin-top: 20px">Sign Up</button>
          </form>
        </div>
        <div class="form-container sign-in-container">
          <form action="#">
            <h1>Sign in</h1>
            <div class="social-container">
              <a href="#" class="social"><i class="fab fa-facebook-f"></i></a>
              <a href="#" class="social"><i class="fab fa-google"></i></a>
              <a href="#" class="social"><i class="fab fa-linkedin-in"></i></a>
            </div>
            <span>or use your account</span>
            <div class="email">
              <input type="email" class="email-input" placeholder="email" />
            </div>
            <div class="password">
              <input
                type="password"
                class="pass-input"
                placeholder="password"
              />
            </div>
            <a href="#">Forgot your password?</a>
            <button>Sign In</button>
          </form>
        </div>
        <div class="overlay-container">
          <div class="overlay">
            <div class="overlay-panel overlay-left">
              <h1>Welcome Back , Admin!</h1>
              <p>Please login with valid credentials</p>
              <button class="ghost" id="signIn">Sign In</button>
            </div>
            <div class="overlay-panel overlay-right">
              <h1>Hello, User!</h1>
              <p>To create account, please enter your details</p>
              <button class="ghost" id="signUp">Sign Up</button>
            </div>
          </div>
        </div>
      </div>
    </div>
    <footer
      class="footer-basic"
      style="background: rgb(0, 0, 0); color: rgb(255, 255, 255)"
